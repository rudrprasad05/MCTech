---
import '../style/main.css'
import Card from '../components/Card.astro';
import Base from "../layouts/base.astro";
import Hero from "../components/Hero.astro";
import Search from '../components/Search.astro';

const all = await Astro.glob('../pages/printers/*.md');

---

<Base 
title="Printers | Mctech Fiji"
description="Explore a range of efficient printers at Mctech Computers Fiji. From versatile all-in-one printers to high-speed laser printers, we offer reliable solutions for your printing needs. Find the perfect printer to produce high-quality documents and images"
keywords="Printers, Mctech Computers Fiji, all-in-one printers, laser printers, efficient printing, reliable solutions, printing needs, high-quality documents, images"
>   
    <h1 class="h1">Our Printer Collection</h1>
    <Search/>
    <div class="grid grid-cols-1 md:grid-cols-4 gap-10 w-4/5 m-auto">
        {all.map(info => <Card { info } />)}
    </div>

    
</Base>